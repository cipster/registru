[#ftl][#import "layouts/layouts.ftl" as layout][#assign security=JspTaglibs["http://www.springframework.org/security/tags"] /][#include "content.ftl"][#include "formMacros.ftl"][@layout.basic]    [@sidebar]    <div class="pull-right">        [@searchbox "registru-ogb-search" "registru-ogb-table"/]        [@security.authorize access="hasAnyRole('ROLE_ADMIN','ROLE_SUPERUSER')"]            <div class="list-group">                [@listItemOpenModal "addRegistru-ogb" "ADD_INREGISTRARE" "modal-addRegistru-ogb" "plus"/]            </div>            <div class="list-group">                [@listItemOpenModal "addTipContinut" "ADD_TIP_CONTINUT" "modal-addTipContinut" "ticket"/]            </div>        [/@security.authorize]    </div>    [/@sidebar]    [@content]            [@ajaxDataTable "registru-ogb-table"]            <th> [@spring.message "REGISTRU.NR_REGISTRU"/] </th>            <th> [@spring.message "REGISTRU.DATA"/] </th>            <th> [@spring.message "REGISTRU.NR_SI_DATA_DOCUMENT"/] </th>            <th> [@spring.message "REGISTRU.FROM"/] </th>            <th> [@spring.message "REGISTRU.TIP_CONTINUT"/] </th>            <th> [@spring.message "REGISTRU.CONTINUT"/] </th>            <th> [@spring.message "REGISTRU.DATA_EXPEDIERE"/] </th>            <th> [@spring.message "REGISTRU.TO"/] </th>            <th> [@spring.message "REGISTRU.PRIMIT_PRIN_POSTA"/] </th>            [/@ajaxDataTable]        [@security.authorize access="hasAnyRole('ROLE_ADMIN','ROLE_SUPERUSER')"]            [@modalForm "modal-addRegistru-ogb" "ADD_INREGISTRARE" "/app/secure/registru/add-ogb" "ADD_INTRARE" "book"]                [#include "dialogs/addOGBForm.ftl"]            [/@modalForm]        [/@security.authorize]    [/@content][/@layout.basic]<script src="/js/static/registruOGB.js"></script>
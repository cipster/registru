[#ftl][#import "layouts/layouts.ftl" as layout][#assign security=JspTaglibs["http://www.springframework.org/security/tags"] /][#include "content.ftl"][#include "formMacros.ftl"][@layout.basic]    [@sidebar]    <div class="pull-right">        [@searchbox "user-search" "user-table" false /]        [@security.authorize access="hasAnyRole('ROLE_ADMIN', 'ROLE_SUPERUSER')"]            <div class="list-group">                [@multiTabListItemOpenModal "addUser" "ADD_USER" "modal-addUser" "user fa-1-5x"/]            </div>            [@security.authorize access="hasAnyRole('ROLE_ADMIN')"]                <div class="list-group">                    [@multiTabListItemOpenModal "editUser" "EDIT_USER" "" "edit fa-1-5x"/]                </div>            [/@security.authorize]            <div class="list-group">                [@multiTabListItemOpenModal "addTipContinut" "ADD_TIP_CONTINUT" "modal-addTipContinut" "ticket fa-1-5x"/]            </div>            [@security.authorize access="hasAnyRole('ROLE_ADMIN')"]                <div class="list-group">                    [@multiTabListItemOpenModal "editTipContinut" "EDIT_TIP_CONTINUT" "" "edit fa-1-5x"/]                </div>                <div class="list-group">                    [@multiTabListItemOpenModal "deleteTipContinut" "DELETE_TIP_CONTINUT" "" "ban fa-1-5x"/]                </div>            [/@security.authorize]            <div class="list-group">                [@multiTabListItemOpenModal "addDestinatar" "ADD_DESTINATAR" "modal-addDestinatar" "users fa-1-5x"/]            </div>            [@security.authorize access="hasAnyRole('ROLE_ADMIN')"]                <div class="list-group">                    [@multiTabListItemOpenModal "editDestinatar" "EDIT_DESTINATAR" "" "edit fa-1-5x"/]                </div>                <div class="list-group">                    [@multiTabListItemOpenModal "deleteDestinatar" "DELETE_DESTINATAR" "" "ban fa-1-5x"/]                </div>            [/@security.authorize]        [/@security.authorize]    </div>    [/@sidebar]    [@content]    <div>        <!-- Nav tabs -->        <ul class="nav nav-tabs" role="tablist">            <li role="presentation" class="active"><a href="#users" aria-controls="users" role="tab"                                                      data-toggle="tab">[@spring.message "USERS"/]</a></li>            <li role="presentation"><a href="#tip-continut" aria-controls="tip-continut" role="tab"                                       data-toggle="tab">[@spring.message "REGISTRU.TIP_CONTINUT"/]</a></li>            <li role="presentation"><a href="#destinatar" aria-controls="destinatar" role="tab"                                       data-toggle="tab">[@spring.message "DESTINATAR"/]</a></li>        </ul>        <!-- Tab panes -->        <div class="tab-content">            <div role="tabpanel" class="tab-pane fade in active" id="users">                [@rowBreak]                    <br>                [/@rowBreak]                [@ajaxDataTable "user-table"]                    <th> [@spring.message "USER.USERNAME"/] </th>                    <th> [@spring.message "USER.LASTNAME"/] </th>                    <th> [@spring.message "USER.NAME"/] </th>                    <th> [@spring.message "USER.MOBILEPHONE"/] </th>                    <th> [@spring.message "USER.WORKPHONE"/] </th>                    <th> [@spring.message "USER.EMAIL"/] </th>                    <th> [@spring.message "USER.FAX"/] </th>                    <th> [@spring.message "USER.LAST_LOGIN"/] </th>                    <th> [@spring.message "USER.LAST_PASS_CHANGE"/] </th>                    <th> [@spring.message "USER.ROLES"/] </th>                    <th> [@spring.message "USER.ACTIV"/] </th>                [/@ajaxDataTable]            </div>            <div role="tabpanel" class="tab-pane fade" id="tip-continut">                [@rowBreak]                    <br>                [/@rowBreak]                [@ajaxDataTable "tip-continut-table"]                    <th width="40" style="max-width: 50px;">Nr. Crt.</th>                    <th> [@spring.message "REGISTRU.TIP_CONTINUT"/] </th>                [/@ajaxDataTable]            </div>            <div role="tabpanel" class="tab-pane fade" id="destinatar">                [@rowBreak]                    <br>                [/@rowBreak]                [@ajaxDataTable "destinatar-table"]                    <th width="40" style="max-width: 50px;">Nr. Crt.</th>                    <th> [@spring.message "DESTINATAR"/] </th>                [/@ajaxDataTable]            </div>        </div>    </div>        [@security.authorize access="hasAnyRole('ROLE_ADMIN', 'ROLE_SUPERUSER')"]            [@modalForm "modal-addUser" "ADD_USER_TITLE" "/app/secure/profil/add-user" "SAVE.CHANGES" "user"]                [#include "dialogs/userForm.ftl"]            [/@modalForm]            [@security.authorize access="hasAnyRole('ROLE_ADMIN')"]                [@modalForm "modal-editUser" "EDIT_USER" "/app/secure/profil/edit-user" "SAVE.CHANGES" "user"]                    [#include "dialogs/userEditForm.ftl"]                [/@modalForm]                [@smallModalForm "modal-editTipContinut" "EDIT_TIP_CONTINUT" "/app/secure/registru/add-tip-continut" "SAVE.CHANGES" "user"]                    [#include "dialogs/editTipContinutForm.ftl"]                [/@smallModalForm]                [@smallModalForm "modal-editDestinatar" "EDIT_DESTINATAR" "/app/secure/registru/add-destinatar" "SAVE.CHANGES" "user"]                    [#include "dialogs/editDestinatarForm.ftl"]                [/@smallModalForm]            [/@security.authorize]        [/@security.authorize]    [/@content][/@layout.basic]<script src="/js/static/admin.js"></script>
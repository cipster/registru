[#ftl][#import "layouts/layouts.ftl" as layout][#assign security=JspTaglibs["http://www.springframework.org/security/tags"] /][#include "content.ftl"][#include "formMacros.ftl"][@layout.basic]    [@sidebar]    <div class="pull-right">        [@searchbox "user-search" "user-table"/]        [@security.authorize access="hasAnyRole('ROLE_ADMIN','ROLE_SUPERUSER')"]            <div class="list-group">                [@listItemOpenModal "addUser" "ADD_USER" "modal-addUser" "plus"/]            </div>        [/@security.authorize]    </div>    [/@sidebar]    [@content]        [@ajaxDataTable "user-table"]        <th> [@spring.message "USER.USERNAME"/] </th>        <th> [@spring.message "USER.LASTNAME"/] </th>        <th> [@spring.message "USER.NAME"/] </th>        <th> [@spring.message "USER.MOBILEPHONE"/] </th>        <th> [@spring.message "USER.WORKPHONE"/] </th>        <th> [@spring.message "USER.EMAIL"/] </th>        <th> [@spring.message "USER.FAX"/] </th>        <th> [@spring.message "USER.LAST_LOGIN"/] </th>        <th> [@spring.message "USER.LAST_PASS_CHANGE"/] </th>        <th> [@spring.message "USER.ROLES"/] </th>        <th> [@spring.message "USER.ACTIV"/] </th>        [/@ajaxDataTable]    [/@content][/@layout.basic]<script src="/js/static/admin.js"></script>
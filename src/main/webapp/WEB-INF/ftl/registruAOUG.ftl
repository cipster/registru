[#ftl][#import "layouts/layouts.ftl" as layout][#assign security=JspTaglibs["http://www.springframework.org/security/tags"] /][#include "content.ftl"][#include "formMacros.ftl"][@layout.basic]    [@sidebar]    <div class="pull-right">        [@searchbox "registru-search" "registru-table"/]        [@security.authorize access="hasAnyRole('ROLE_ADMIN','ROLE_SUPERUSER')"]            <div class="list-group">                [@listItemOpenModal "addRegistru" "ADD_INREGISTRARE" "modal-addRegistru" "plus"/]            </div>            <div class="list-group">                [@listItemOpenModal "addTipContinut" "ADD_TIP_CONTINUT" "modal-addTipContinut" "ticket"/]            </div>        [/@security.authorize]    </div>    [/@sidebar]    [@content]        [@security.authorize access="hasAnyRole('ROLE_ADMIN','ROLE_SUPERUSER')"]            [@ajaxDataTable "registru-table"]            <th> [@spring.message "REGISTRU.NR_REGISTRU"/] </th>            <th> [@spring.message "REGISTRU.DATA"/] </th>            <th> [@spring.message "REGISTRU.NR_SI_DATA_DOCUMENT"/] </th>            <th> [@spring.message "REGISTRU.FROM"/] </th>            <th> [@spring.message "REGISTRU.TIP_CONTINUT"/] </th>            <th> [@spring.message "REGISTRU.CONTINUT"/] </th>            <th> [@spring.message "REGISTRU.DATA_EXPEDIERE"/] </th>            <th> [@spring.message "REGISTRU.TO"/] </th>            <th> [@spring.message "REGISTRU.SEMNAT"/] </th>            <th> [@spring.message "REGISTRU.PRIMIT_PRIN_POSTA"/] </th>            [/@ajaxDataTable]            [@modalForm "modal-addRegistru" "ADD_INREGISTRARE" "/app/secure/registru/add-intrare" "ADD_INTRARE" "book"]                [#include "dialogs/addInregistrareForm.ftl"]            [/@modalForm]            [@smallModalForm "modal-addTipContinut" "ADD_TIP_CONTINUT" "/app/secure/registru/add-tip-continut" "ADD_INTRARE" "ticket"]                [#include "dialogs/addTipContinutForm.ftl"]            [/@smallModalForm]        [/@security.authorize]    [/@content][/@layout.basic]<script src="/js/static/registruAOUG.js"></script>